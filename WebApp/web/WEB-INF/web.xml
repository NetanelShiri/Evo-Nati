<?xml version="1.0" encoding="UTF-8"?>
<web-app version="3.0" xmlns="http://java.sun.com/xml/ns/javaee" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://java.sun.com/xml/ns/javaee http://java.sun.com/xml/ns/javaee/web-app_3_0.xsd">
    <servlet>
        <servlet-name>DataServlet</servlet-name>
        <servlet-class>ett.servlets.DataServlet</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>AlgoServlet</servlet-name>
        <servlet-class>ett.servlets.AlgoServlet</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>LoginShortResponseServlet</servlet-name>
        <servlet-class>ett.servlets.LightweightLoginServlet</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>PresentData</servlet-name>
        <servlet-class>ett.servlets.PresentDataServlet</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>FilesListServlet</servlet-name>
        <servlet-class>ett.servlets.FileListServlet</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>UsersListServlet</servlet-name>
        <servlet-class>ett.servlets.UsersListServlet</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>AlgoServlet</servlet-name>
        <url-pattern>/pages/fileroom/fileAlgo</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>LoginShortResponseServlet</servlet-name>
        <url-pattern>/loginShortResponse</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>PresentData</servlet-name>
        <url-pattern>/pdata</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>FilesListServlet</servlet-name>
        <url-pattern>/fileslist</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>UsersListServlet</servlet-name>
        <url-pattern>/userslist</url-pattern>
    </servlet-mapping>
</web-app>
